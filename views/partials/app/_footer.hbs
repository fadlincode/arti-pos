        <script>
            $(document).ready(function() {
                $('.updateLikeBtn').click(function() {
                    var article = parseInt($(this).data('id'));

                    $.ajax({
                        type: 'POST',
                        data: {id: article},
                        url: '/updatelikes',
                        success: function(response) {
                            console.log(response.article.like_count);
                            $('[data-id="' + article + '"].likeCount').text(response.article.like_count);
                        },
                        error: function(error) {
                            console.error('Error updating likes:', error);
                        }
                    });
                });
            });
        </script>
